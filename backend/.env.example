APP_CORS_ALLOW_ORIGINS=*

# SQLite（相对 backend 目录）
APP_SQLITE_PATH=data/app.db

# LLM（也可在前端“设置”里通过 /config/llm 保存到 SQLite 覆盖）
APP_LLM_BASE_URL=https://api.openai.com
APP_LLM_API_KEY=
APP_LLM_MODEL=gpt-4o-mini
APP_LLM_TEMPERATURE=0.2

# Neo4j
APP_NEO4J_URI=bolt://localhost:7687
APP_NEO4J_USER=neo4j
APP_NEO4J_PASSWORD=neo4j_password

